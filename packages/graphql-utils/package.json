  "dependencies": {
    "@luminate/mongo": "*",
    "apollo-server-express": "^2.9.13",
    "dataloader": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongoose": "^5.5.36",
    "@types/uuid": "^3.4.6"
  }
}
