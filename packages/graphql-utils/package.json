  "main": "dist/index.js",
  "types": "src/index.ts",
  "author": "Bryan Kegley <hi@bryankegley.me>",
  "license": "MIT",
  "scripts": {
    "develop": "nodemon --watch src/ -e ts --exec \"npm run start\"",
    "build": "yarn generate-scopes && rimraf ./dist && tsc",
